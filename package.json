    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "innersvg-polyfill": "^0.0.2",
    "poststylus": "^1.0.0",
    "pug": "^2.0.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "storybook-addon-vue-info": "^0.2.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
